#ifndef INC_PARSER_TASK_H_
#define INC_PARSER_TASK_H_

#include "stdint.h"
#include "klebot_commands.h"

typedef struct
{
	uint8_t data[MAX_COMMAND_LENGTH];
	uint8_t length;
	uint8_t origin;
}Parser_Frame_t;

typedef enum
{
	RADIO_SOURCE,
	UART_SOURCE,
	MENU_SOURCE
}Parser_Origin_t;

#endif /* INC_PARSER_TASK_H_ */
