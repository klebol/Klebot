#ifndef INC_KLEBOT_PARSER_H_
#define INC_KLEBOT_PARSER_H_

#include "main.h"
#include "klebot_commands.h"
#include "klebot_radio.h"

typedef enum
{
	INTERNAL,	//commands from robot itself (buttons)
	RADIO,		//comands from radio (nrf)
	UART,		//comands via uart
	USB			//commands from usb com port
}Parser_DataSource_t;



void Parser_Klebot(uint8_t *command, uint8_t length);

#endif /* INC_KLEBOT_PARSER_H_ */
