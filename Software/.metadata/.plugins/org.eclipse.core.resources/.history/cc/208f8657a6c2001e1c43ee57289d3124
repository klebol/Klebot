#ifndef INC_PID_H_
#define INC_PID_H_

#include "stdint.h"

typedef struct
{
	float kp;
	float ki;
	float kd;
	int16_t LastError;
	int32_t IntegralError;
	int16_t Output;

	int32_t MaxIntegralError;
	int16_t MaxOutput;
}PID_Controller_t;


#endif /* INC_PID_H_ */
